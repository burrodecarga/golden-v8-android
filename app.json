{
  "expo": {
    "name": "Golden Rule Cargo LLC",
    "slug": "golden-rule-cargo-llc-8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icono.png",
    "scheme": "goldenv8",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icono.png",
        "backgroundImage": "./assets/images/icono.png",
        "monochromeImage": "./assets/images/icono.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.edwinhenriquezh.goldenv8"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icono.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1b67c2c-9b3b-4672-9fe3-f154491f48aa"
      }
    }
  }
}